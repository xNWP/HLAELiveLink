// C4D-DialogResourceDIALOG DLG_HLL{	NAME DLG_HLL_TITLE;	SCALE_H;SCALE_V;  	GROUP	{		BORDERSIZE 4,4,4,4;		COLUMNS 1;				GROUP		{			COLUMNS 5;			BORDERSIZE 0,0,0,6;						BUTTON BTN_LISTEN { SIZE 150, 0; }						STATICTEXT { NAME STR_HOSTNAME; }			EDITTEXT TXT_HOSTNAME { SIZE 200, 0; }						STATICTEXT { NAME STR_PORT; }			EDITTEXT TXT_PORT { SIZE 60, 0; }		}				GROUP		{			COLUMNS 2;						BUTTON BTN_SETCAMERA { NAME STR_SETCAMERA; }						GROUP			{				COLUMNS 2;				BORDERSIZE 6,0,0,0;								STATICTEXT { NAME STR_COORDINATE_SYSTEM; }				COMBOBOX CBX_COORDINATE_SYSTEM				{					SIZE 60, 0;										CHILDS					{						CBO_Y_UP, STR_Y_UP;						CBO_NZ_UP, STR_NZ_UP;					}				}			}		}				GROUP		{			BORDERSIZE 0,0,0,8;			STATICTEXT TXT_CAMERANAME { SIZE 500, 0; }		}				GROUP		{			COLUMNS 1;			BORDERSIZE 24,4,24,4;			BORDERSTYLE BORDER_ACTIVE_1;						STATICTEXT { NAME STR_MAPPING; }			GROUP			{				COLUMNS 3;								STATICTEXT { NAME STR_CINEMA4D; }				BUTTON BTN_MAPPING { SIZE 30, 0; }				STATICTEXT { NAME STR_GAME; }						}		}				GROUP		{			BORDERSTYLE BORDER_ACTIVE_4;			SCALE_H;			BORDERSIZE 2,2,2,2;			ALIGN_BOTTOM;						STATICTEXT TXT_STATUS { SIZE 500, 0; }		}	}}